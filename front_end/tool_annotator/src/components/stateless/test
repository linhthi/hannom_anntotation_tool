const getImages = async () => {
    let res = await axios(API.GETPAGES + `?id=${id}`)
    if (res.data.success) {
        let res = await res.data.data
        setPages(res);
        setImages(formatInput(res));
    } else {
        navigate(ROUTE.LOGIN);
      }
  }
useEffect(() => {
    getImages()
  }, []);